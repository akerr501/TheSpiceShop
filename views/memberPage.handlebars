{{!-- Members page, displays Member table --}}

<main class="members-container">
{{> table title="Members" map=members}}

<br><br>
<form>
  <label for="fname">First name:</label><br>
  <input type="text" id="fname" name="fname"><br>
  <label for="mname">Middle name (optional):</label><br>
  <input type="text" id="mname" name="mname"><br>
  <label for="lname">Last name:</label><br>
  <input type="text" id="lname" name="lname"><br>
  <label for="households">Household ID:</label>
  <select name="households" id="memmber-dropdown">
    {{#each householdIDs}}
      <option value="{{this}}">{{this}}</option>
    {{/each}}
  </select><br><br>
  <input type="submit" value="Submit">
</form>
</main>
